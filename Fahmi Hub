local DiscordWebhookURL = "https://discord.com/api/webhooks/1458491907138392297/zQAe_M5KL_BCpj-ftkxSU-Yn8o8EeLsZgnm7uzfUXQTjKAcLDDvGAIiS5YbYsnfqic25" -- Masukkan Webhookmu

local function IsWhitelisted(name)
    for _, user in pairs(WhitelistedUsers) do
        if user == name then return true end
    end
    return false
end

local IsOwner = (PlayerName == OwnerName)

